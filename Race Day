let raceNumber = Math.floor(Math.random() * 1000);
let isRegistered = true;
let age = 20;
if (isRegistered=true && age>18){
  raceNumber+=1000;
}

if(age>18 && isRegistered){
   console.log(`Runner number ${raceNumber} your race will start at 9:30AM`);
}
else if(age>18 && isRegistered===false){
  console.log(`Runner number ${raceNumber} your race will start at 11:00AM`)
}
else if(age<18 && isRegistered === true){
  console.log(`Runner number ${raceNumber} your race will start at 12:30AM`)
}else{'Please visit registration desk!';}
